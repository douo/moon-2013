&lt;li&gt;&#x8BBE;&#x7F6E;&#x6210;Window&#x7684;rootViewController(iOS 4.0&#x4E4B;&#x524D;UIWindow&#x5E76;&#x6CA1;&#x6709;rootViewController&#x5C5E;&#x6027;,&#x53EA;&#x80FD;&#x901A;&#x8FC7;addSubview&#x7684;&#x65B9;&#x5F0F;&#x6DFB;&#x52A0;&#x4E00;&#x4E2A;View Controller&#x7684;view)&lt;/li&gt;
